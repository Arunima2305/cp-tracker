{
  "manifest_version": 3,
  "name": "CP Dashboard Adder",
  "version": "1.0",
  "description": "Add CP questions to your dashboard from any site!",
  
   
  "action": {
    "default_popup": "popup.html"
  },
  

  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
